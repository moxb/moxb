{
  "name": "@moxb/semui",
  "version": "0.2.0-beta.22",
  "description": "moxb semantic-ui bindings",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "dist",
  "typings": "dist/",
  "files": [
    "dist/**/*",
    "src/**/*.less"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moxb/moxb.git"
  },
  "author": "moxb (moxb.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moxb/moxb/issues"
  },
  "homepage": "https://github.com/moxb/moxb#readme",
  "keywords": [
    "mobx",
    "semantic-ui-react",
    "react",
    "UI"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "../../jest/jestAdapter.js",
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(css|less)$": "../../jest/jestMockStyle.js",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>/node_modules"
    ],
    "testMatch": [
      "**/src/**/__tests__/*.(ts|tsx)"
    ]
  },
  "peerDependencies": {
    "@moxb/moxb": ">0.2.0-beta.9",
    "mobx": "^4.3.0 || ^5.0.0",
    "mobx-react": "^5.2.3",
    "semantic-ui-react": "^0.82.1",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@moxb/moxb": "^0.2.0-beta.22",
    "@types/enzyme": "^3.9.0",
    "@types/jest": "^24.0.11",
    "@types/marked": "^0.6.3",
    "@types/meteor": "^1.4.27",
    "@types/react": "^16.8.8",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "jsdom": "^14.0.0",
    "mobx": "=5.7.0",
    "mobx-react": "^5.4.3",
    "pretty": "^2.0.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "semantic-ui-react": "^0.86.0"
  },
  "dependencies": {
    "marked": "^0.6.1"
  }
}
