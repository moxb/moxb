{
  "name": "@moxb/semui",
  "version": "0.2.0-beta.8",
  "description": "moxb semantic-ui bindings",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "dist",
  "typings": "dist/",
  "files": [
    "dist/**/*",
    "src/**/*.less"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moxb/moxb.git"
  },
  "author": "moxb (moxb.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moxb/moxb/issues"
  },
  "homepage": "https://github.com/moxb/moxb#readme",
  "keywords": [
    "mobx",
    "semantic-ui-react",
    "react",
    "UI"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "../../jest/jestAdapter.js",
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(css|less)$": "../../jest/jestMockStyle.js",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>/node_modules"
    ],
    "testMatch": [
      "**/src/**/__tests__/*.(ts|tsx)"
    ]
  },
  "peerDependencies": {
    "@moxb/moxb": ">0.2.0-beta.8",
    "mobx": "^4.3.0 || ^5.0.0",
    "mobx-react": "^5.2.3",
    "semantic-ui-react": "^0.82.1"
  },
  "devDependencies": {
    "@moxb/moxb": "^0.2.0-beta.8",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.5",
    "@types/marked": "^0.4.2",
    "@types/meteor": "^1.4.22",
    "@types/react": "^16.4.16",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "jsdom": "^12.2.0",
    "mobx": "^4.3.0 || ^5.0.0",
    "mobx-react": "^5.2.8",
    "pretty": "^2.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "semantic-ui-react": "^0.82.5"
  },
  "dependencies": {
    "marked": "^0.5.1"
  }
}
