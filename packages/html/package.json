{
  "name": "@moxb/html",
  "version": "0.2.0-beta.93",
  "description": "moxb html bindings",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "dist",
  "typings": "dist/",
  "files": [
    "dist/**/*",
    "src/**/*.less"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moxb/moxb.git"
  },
  "author": "moxb (moxb.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moxb/moxb/issues"
  },
  "homepage": "https://github.com/moxb/moxb#readme",
  "keywords": [
    "mobx",
    "html",
    "react",
    "UI"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "../../jest/jestAdapter.js",
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(css|less)$": "../../jest/jestMockStyle.js",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>/node_modules"
    ],
    "testMatch": [
      "**/src/**/__tests__/*.(ts|tsx)"
    ]
  },
  "peerDependencies": {
    "@moxb/moxb": ">0.2.0-beta.93",
    "mobx": "^6.5.0",
    "mobx-react": "^7.3.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@moxb/moxb": "^0.2.0-beta.93",
    "@types/enzyme": "3.10.12",
    "@types/jest": "26.0.23",
    "@types/react-test-renderer": "17.0.2",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "mobx": "6.5.0",
    "pretty": "2.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "2.3.1",
    "uuid": "8.3.2"
  }
}
