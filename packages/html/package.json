{
  "name": "@moxb/html",
  "version": "0.2.0-beta.30",
  "description": "moxb html bindings",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "dist",
  "typings": "dist/",
  "files": [
    "dist/**/*",
    "src/**/*.less"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moxb/moxb.git"
  },
  "author": "moxb (moxb.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moxb/moxb/issues"
  },
  "homepage": "https://github.com/moxb/moxb#readme",
  "keywords": [
    "mobx",
    "html",
    "react",
    "UI"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "../../jest/jestAdapter.js",
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(css|less)$": "../../jest/jestMockStyle.js",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>/node_modules"
    ],
    "testMatch": [
      "**/src/**/__tests__/*.(ts|tsx)"
    ]
  },
  "peerDependencies": {
    "@moxb/moxb": ">0.2.0-beta.9",
    "mobx": "^5.10.0",
    "mobx-react": "^6.0.3",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@moxb/moxb": "^0.2.0-beta.30",
    "@types/enzyme": "3.9.3",
    "@types/jest": "24.0.13",
    "@types/react-test-renderer": "16.8.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "mobx": "5.10.0",
    "mobx-react": "6.0.3",
    "pretty": "2.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "tslib": "1.9.3"
  }
}
