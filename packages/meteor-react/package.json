{
  "name": "@moxb/meteor-react",
  "version": "0.3.81",
  "description": "MobX meteor React binding",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "make all"
  },
  "main": "dist",
  "typings": "dist/",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moxb/moxb.git"
  },
  "author": "moxb (moxb.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moxb/moxb/issues"
  },
  "homepage": "https://github.com/moxb/moxb#readme",
  "keywords": [
    "react",
    "meteor"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "node",
    "testMatch": [
      "**/src/**/__tests__/*.(ts|tsx)"
    ]
  },
  "dependencies": {
    "@moxb/meteor": "^0.3.81",
    "@moxb/moxb": "^0.3.80"
  },
  "peerDependencies": {
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.3.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/jest": "27.5.0",
    "@types/meteor": "2.7.1",
    "@types/react": "17.0.2",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "eslint": "8.14.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "mongo-mock": "4.0.0"
  },
  "gitHead": "c0ed29fe8703cdd8bd26b1be4dd8c2f42390fea4"
}
