    "@types/jest": "24.0.13",
    "@types/meteor": "1.4.28",
    "@types/uuid": "3.4.4",
    "mobx": "5.10.0",
    "mobx-react": "6.0.3",
    "mongo-mock": "3.7.1",
    "tslib": "1.9.3"
  },
  "dependencies": {
    "uuid": "3.3.2"
  }
