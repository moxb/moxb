{
  "name": "@moxb/antd",
  "version": "0.3.1",
  "description": "moxb ant design bindings",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "dist",
  "typings": "dist/",
  "files": [
    "dist/**/*",
    "src/**/*.less"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moxb/moxb.git"
  },
  "author": "moxb (moxb.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moxb/moxb/issues"
  },
  "homepage": "https://github.com/moxb/moxb#readme",
  "keywords": [
    "mobx",
    "antd",
    "ant-design",
    "react",
    "UI"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "../../jest/jestAdapter.js",
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(css|less)$": "../../jest/jestMockStyle.js",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>/node_modules"
    ],
    "testMatch": [
      "**/src/**/__tests__/*.test.(ts|tsx)"
    ]
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@moxb/moxb": "^0.2.0-beta.93",
    "antd": "^4.19.5",
    "mobx": "^6.5.0",
    "react": "^17.0.2",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@moxb/moxb": "^0.3.1",
    "@moxb/react-html": "^0.3.1",
    "@types/enzyme": "3.10.12",
    "@types/jest": "26.0.23",
    "@types/react": "^17.0.2",
    "@types/react-test-renderer": "^17.0.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.14.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "pretty": "2.0.0"
  },
  "gitHead": "350ec1fe18b9368d544fb305cc5de43a86178ef9"
}
