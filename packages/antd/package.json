{
  "name": "@moxb/antd",
  "version": "0.2.0-beta.72",
  "description": "moxb ant design bindings",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "dist",
  "typings": "dist/",
  "files": [
    "dist/**/*",
    "src/**/*.less"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moxb/moxb.git"
  },
  "author": "moxb (moxb.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moxb/moxb/issues"
  },
  "homepage": "https://github.com/moxb/moxb#readme",
  "keywords": [
    "mobx",
    "antd",
    "ant-design",
    "react",
    "UI"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "../../jest/jestAdapter.js",
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(css|less)$": "../../jest/jestMockStyle.js",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "modulePaths": [
      "<rootDir>/node_modules"
    ],
    "testMatch": [
      "**/src/**/__tests__/*.test.(ts|tsx)"
    ]
  },
  "peerDependencies": {
    "@ant-design/icons": "4.2.2",
    "@moxb/moxb": ">0.2.0-beta.40",
    "antd": "4.6.3",
    "mobx": "^5.15.6",
    "mobx-react": "^6.3.0",
    "tslib": "^2.0.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@ant-design/icons": "4.2.2",
    "@moxb/html": "^0.2.0-beta.72",
    "@moxb/moxb": "^0.2.0-beta.72",
    "@types/enzyme": "3.10.5",
    "@types/jest": "26.0.13",
    "@types/react": "16.9.49",
    "@types/react-test-renderer": "16.9.3",
    "@types/uuid": "^8.3.0",
    "antd": "4.4.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "enzyme-to-json": "3.5.0",
    "mobx": "5.13.1",
    "pretty": "2.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "tslib": "1.10.0"
  },
  "dependencies": {
    "marked": "1.1.1"
  }
}
