{
  "name": "@moxb/moxb",
  "version": "0.2.0-beta.24",
  "description": "MobX data binding",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "dist",
  "typings": "dist/",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moxb/moxb.git"
  },
  "author": "moxb (moxb.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moxb/moxb/issues"
  },
  "homepage": "https://github.com/moxb/moxb#readme",
  "keywords": [
    "mobx",
    "UI"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "node",
    "testMatch": [
      "**/src/**/__tests__/*.(ts|tsx)"
    ],
    "modulePaths": [
      "<rootDir>/peer-dependencies/node_modules"
    ]
  },
  "peerDependencies": {
    "mobx": "^4.3.0 || ^5.0.0",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@types/atob": "2.1.2",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.11",
    "@types/urijs": "^1.15.38",
    "mobx": "=5.7.0"
  },
  "dependencies": {
    "btoa-lite": "^1.0.0",
    "history": "^4.7.2",
    "urijs": "^1.19.1"
  }
}
